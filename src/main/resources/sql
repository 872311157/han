CREATE TABLE hrole(
	id serial PRIMARY KEY not null,
	createtime DATE,
	isDel INT,
	roleName VARCHAR(64),
	roleNameZh VARCHAR(128)
);
select * from hrole
insert into hrole(createtime, isDel, roleName, roleNameZh) values('2021-02-25 10:00:00', 0, 'ROLE_ADMIN', '管理员')






